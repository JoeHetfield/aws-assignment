{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["react-intl", {
      "messagesDir": "./app/i18n",
      "enforceDescriptions":true
    }]
  ]
}
